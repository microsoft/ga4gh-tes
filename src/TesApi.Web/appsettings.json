{
  "Logging": {
    "LogLevel": {
      "Azure": "Warning",
      "Microsoft": "Warning",
      "TesApi.Web.AzureProxy": "Debug",
      "TesApi.Web.BatchPool": "Debug",
      "TesApi.Web.BatchScheduler": "Debug",
      "TesApi.Web.PoolScheduler": "Debug",
      "TesApi.Web.TaskScheduler": "Debug",
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "BatchNodes": {
    "DisablePublicIpAddress": "True"
  },
  "BatchScheduling": {
    "Disable": "False",
    "UsePreemptibleVmsOnly": "false",
    "UseLegacyAutopools": "false",
    "PoolRotationForcedDays": "7"
  },
  "BatchImageGen2": {
    "Offer": "ubuntu-hpc",
    "Publisher": "microsoft-dsvm",
    "Sku": "2004",
    "Version": "latest",
    "NodeAgentSkuId": "batch.node.ubuntu 20.04"
  },
  "BatchImageGen1": {
    "Offer": "ubuntu-server-container",
    "Publisher": "microsoft-azure-batch",
    "Sku": "20-04-lts",
    "Version": "latest",
    "NodeAgentSkuId": "batch.node.ubuntu 20.04"
  },
  "BatchAccount": {
    "AccountName": ""
  },
  "TesPostgreSql": {
    "ServerName": "",
    "ServerNameSuffix": ".postgres.database.azure.com",
    "ServerPort": "5432",
    "ServerSslMode": "VerifyFull",
    "DatabaseName": "tes_db",
    "DatabaseUserLogin": "",
    "DatabaseUserPassword": ""
  },
  "Martha": {
    "CromwellDrsLocalizer": "broadinstitute/cromwell-drs-localizer:develop"
  },
  "RetryPolicy": {
    "MaxRetryCount": 5,
    "ExponentialBackOffExponent": 2
  }
}
